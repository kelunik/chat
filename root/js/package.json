{
  "name": "amp-chat",
  "version": "1.0.0",
  "author": "Niklas Keller <me@kelunik.com>",
  "contributors": [],
  "scripts": {
    "install": "mkdir -p ../../html/node_modules && cp -r node_modules/handlebars ../../html/node_modules/handlebars && cp -r node_modules/hbsfy ../../html/node_modules/hbsfy",
    "build": "browserify -t hbsfy main.js | uglifyjs -m -c sequences,properties,dead_code,drop_debugger=false,conditionals,evaluate,booleans,loops,if_return,join_vars,drop_console=false,unused=false > all.min.js",
    "watch": "watchify -t hbsfy main.js -o all.min.js --verbose"
  },
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdlowrey/amp-chat"
  },
  "bugs": {
    "url": "https://github.com/rdlowrey/amp-chat/issues"
  },
  "dependencies": {
    "autolinker": "0.15.x",
    "browserify": "8.1.x",
    "handlebars": "2.0.x",
    "hbsfy": "2.2.x",
    "minifyify": "6.1.x",
    "mocha": "2.1.x",
    "moment": "2.9.x",
    "remarkable": "1.6.x",
    "remarkable-regexp": "0.2.4",
    "uglify-js": "2.4.x",
    "watchify": "2.3.x"
  },
  "devDependencies": {},
  "preferGlobal": false,
  "private": true
}
