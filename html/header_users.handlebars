{{#each this}}
    <a class="ac-header-member" href="https://github.com/{{name}}" target="_blank" draggable="true"
       ondragstart="event.dataTransfer.setData('text/plain', '@{{name}}')">
        <div class="ac-header-member-avatar"
             style="background-image: url('{{{avatar avatar}}}');"
             title="{{name}}">
                    <span class="user-activity user-activity-{{id}}" data-state="{{state}}"
                          title="{{activity-state state}}"></span>
        </div>
    </a>
{{/each}}
