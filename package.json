{
  "name": "amp-chat",
  "version": "1.0.0",
  "author": "Niklas Keller <me@kelunik.com>",
  "contributors": [],
  "scripts": {
    "build": "travis/env.sh browserify -t hbsfy root/js/main.js | travis/env.sh uglifyjs -m -c sequences,properties,dead_code,drop_debugger=false,conditionals,evaluate,booleans,loops,if_return,join_vars,drop_console=false,unused=false > root/js/all.min.js",
    "watch": "travis/env.sh watchify -t hbsfy root/js/main.js -o root/js/all.min.js --verbose"
  },
  "main": "root/js/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdlowrey/amp-chat"
  },
  "bugs": {
    "url": "https://github.com/rdlowrey/amp-chat/issues"
  },
  "dependencies": {
    "autolinker": "0.15.x",
    "browserify": "8.1.x",
    "chai": "^2.0.0",
    "handlebars": "2.0.x",
    "hbsfy": "2.2.x",
    "jsdom": "^3.1.1",
    "minifyify": "6.1.x",
    "mocha": "2.1.x",
    "mocha-jsdom": "^0.2.0",
    "mocha-sinon": "^1.1.4",
    "moment": "2.9.x",
    "remarkable": "1.6.x",
    "remarkable-regexp": "0.2.4",
    "sinon": "^1.12.2",
    "uglify-js": "2.4.x",
    "watchify": "2.3.x"
  },
  "devDependencies": {},
  "preferGlobal": false,
  "private": true
}
